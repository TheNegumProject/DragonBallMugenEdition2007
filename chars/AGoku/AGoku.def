; Definition file for Another Goku
; Contains all the filenames needed for the character

; Player information
[Info]
name = "Another Goku by HyperionX & Iron Mask"        ;Name of character
displayname = "Goku" ;Name of character to display
versiondate = 04,14,2001    ;Version date of character
mugenversion = 04,14,2001   ;Version of M.U.G.E.N character works on
author = "HyperionX & Iron Mask"         ;Character author name
pal.defaults =  1 ;Default palettes in order of preference

; Files for the player
[Files]
cmd     = gcmd.cmd   ;Command set
cns     = gcns.cns   ;Constants
st      = gcns.cns   ;States
stcommon = common1.cns ;Common states
sprite  = gsff.sff   ;Sprite
anim    = gair.air   ;Animation
sound   = gsnd.snd   ;Sound
pal1    = 1.act  ;Palettes (can have up to 12)

