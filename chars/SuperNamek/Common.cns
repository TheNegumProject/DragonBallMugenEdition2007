;Stand
[Statedef 0]
type 			= S
movetype		= I
physics		= S
;anim			= -1
;velset		= 0,0
ctrl			= 1
poweradd		= 0
juggle		= 0
facep2		= 0
hitdefpersist	= 0
movehitpersist	= 0
hitcountpersist	= 0
sprpriority		= 0

[State 0: 		ChangeAnim]
type 			= ChangeAnim
triggerall 		= Life > (Const(Data.Life)/4)
trigger1 		= (Anim != 0 && Anim != 5) || (Anim = 5 && AnimTime = 0)
value 		= 0

[State 0: 		ChangeAnim]
type 			= ChangeAnim
triggerall 		= Life <= (Const(Data.Life)/4)
trigger1 		= (Anim != 5300 && Anim != 5) || (Anim = 5 && AnimTime = 0)
value 		= 5300

[State 0:		CtrlSet]
type			= CtrlSet
trigger1		= RoundState = 4
value			= 0

[State 0:		VarSet]
type			= VarSet
trigger1		= Time = 0
v			= 20
value			= 0

[State 0: 		VelSet]
type 			= VelSet
trigger1 		= Time = 0
y 			= 0

[State 0: 		VelSet] 
type 			= VelSet
trigger1 		= Abs(Vel X) < 2
trigger2 		= Time = 4
x 			= 0

[State 0:		ChangeState]
type			= ChangeState
trigger1		= BackEdgeBodyDist <= -10
value 		= IfElse(Pos Y >= 0,100,8171)

[State 0:		ChangeState]
type			= ChangeState
trigger1		= Pos Y < 0
value			= IfElse(Var(31) != 2,8171,50)

;Goto Vism walk if CC is on.
[State 0: 		ChangeState]
type 			= ChangeState
trigger1 		= Var(9) > 0
value 		= 8231
ctrl 			= 1

