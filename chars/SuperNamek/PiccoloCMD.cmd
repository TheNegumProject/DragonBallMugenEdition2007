;===========================================================================
;-| Required |--------------------------------------------------------------

;---------------------------------------------------------------------------
;Run Fwd
[State -1: 		Run Fwd]
type 			= ChangeState
value 		= 100
trigger1 		= (var(51) = 0) && (command = "FF") && (statetype = S && ctrl)
trigger2		= (var(51) = 1) && (var(34) = 0) && (p2dist x > 100) && (statetype = S && ctrl)

;---------------------------------------------------------------------------
;Run Back
[State -1: 		Run Back]
type 			= ChangeState
value 		= 105
trigger1 		= (var(51) = 0) && (command = "BB") && (statetype = S && ctrl)

;---------------------------------------------------------------------------
;Jump
[State -1: 		Jump]
type 			= ChangeState
value 		= 40
triggerall 		= (command = "holdup") 
trigger1		= (statetype = S && ctrl) || ((stateno = [100,105]) && (backedgebodydist > 0))

;---------------------------------------------------------------------------
;High Jump (Variable Mode ONLY)
[State -1: 		HighJump]
type 			= ChangeState
value 		= 41
triggerall 		= (command = "HighJump")
trigger1		= (statetype = S && ctrl) || ((stateno = [100,105]) && (backedgebodydist > 0))

;High Jump: After CSP (Combination/Variable Mode)
[State -1: 		HighJump]
type 			= ChangeState
value			= 41
ignorehitpause	= 1
triggerall		= (command = "holdup") && (var(31) != 2)
trigger1		= (stateno = 420 && movehit) && (pos y >= 0)

;---------------------------------------------------------------------------
;Long Jump (Combination/Balanced Mode)
[State -1: 		LongJump]
type 			= ChangeState
value 		= 40
triggerall 		= (command = "LongJump")
trigger1		= (statetype = S && ctrl) || ((stateno = [100,105]) && (backedgebodydist > 0)) || ((stateno = [200,250]) && (movecontact && animelem = 2, < 2))

;---------------------------------------------------------------------------
;Wall Jump
[State -1: 		WallJump]
type 			= ChangeState
value 		= 40
triggerall 		= (var(51) = 0) && (var(31) = 0) && (statetype = A && ctrl)
trigger1		= ((command = "LongJump") && (command = "holdfwd") && (backedgebodydist <= 10) && (backedgebodydist > -10)) 
trigger2		= ((command = "LongJump") && (command = "holdback") && (frontedgebodydist <= 10) && (frontedgebodydist > -10)) 

