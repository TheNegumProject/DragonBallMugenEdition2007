[State 40, 6]
type = VelSet
trigger1 = AnimTime = 0
trigger1 = prevstateno = 100 ;RUN_FWD
trigger1 = sysvar(1) = 1
x = const(velocity.runjump.fwd.x)

[State 40, ChangeState]
type = ChangeState
trigger1 = (Command = "sjump1") && (Time = 0) && (var(9) = 1)
trigger2 = (Command = "sjump2") && (Time = 0) && (var(9) = 1)
trigger3 = (Command = "sjump3") && (Time = 0) && (var(9) = 1)
value = 1117
ctrl = 1

[State 40, 7]
type = ChangeState
trigger1 = AnimTime = 0
value = 50
ctrl = 1

